#include<stdio.h>
int main()

{

int t;

scanf("%d",&t);

while(tâ€“)

{

int n1,n2,i,f=0,k=0;

scanf("%d",&n1);

int a[n1];

for(i=0;i<n1;i++)

{

  scanf("%d",&a[i]);

}

scanf("%d",&n2);

int b[n2];

for(i=0;i<n2;i++)

  scanf("%d",&b[i]);

for(i=0;i<n1&&k<n2;i++)

{

if(a[i]==b[k])

{

f++;

k++;

if(f==n2)

break;

}

}

if(f>=n2)
printf("Yes\n");
else
printf("No\n");
}
return 0;
}
