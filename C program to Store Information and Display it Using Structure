#include <stdio.h>
int main()

{

  struct student

  {

    char name[80];

    int rollno;

    float marks;

  };

  struct student s;

  scanf("%s",s.name);

  scanf("%d",&s.rollno);

  scanf("%f",&s.marks);

  printf("Name=%s\n",s.name);

  printf("Roll number=%d\n",s.rollno);

  printf("Marks=%.1f\n",s.marks);

  return 0;

}
